{
  "name": "ooura",
  "version": "2.1.3",
  "description": "Ultra-fast real/complex FFT with simple interface",
  "repository": "https://github.com/audioplastic/ooura",
  "main": "ooura.js",
  "scripts": {
    "test": "xo && jasmine",
    "cover": "istanbul cover jasmine",
    "coveralls": "istanbul cover jasmine && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "xo": {
    "rules": {
      "one-var": 0,
      "no-mixed-operators": 0
    },
    "ignores": [
      "spec/**/*.js"
    ]
  },
  "keywords": [
    "fft",
    "ooura",
    "audio",
    "ifft",
    "stft",
    "dsp",
    "overlap-add",
    "ola",
    "real",
    "complex"
  ],
  "author": "Nick Clark",
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "jasmine": "^2.8.0",
    "xo": "^0.18.2"
  }
}
